<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PanelBgBrush"
                     Color="{DynamicResource PanelBg}" />

    <SolidColorBrush x:Key="CardBgBrush"
                     Color="{DynamicResource CardBg}" />

    <SolidColorBrush x:Key="TextPrimaryBrush"
                     Color="{DynamicResource TextPrimary}" />

    <SolidColorBrush x:Key="TextMutedBrush"
                     Color="{DynamicResource TextMuted}" />

    <SolidColorBrush x:Key="AccentCyanBrush"
                     Color="{DynamicResource AccentCyan}" />

    <SolidColorBrush x:Key="AccentGreenBrush"
                     Color="{DynamicResource AccentGreen}" />

    <SolidColorBrush x:Key="AccentPurpleBrush"
                     Color="{DynamicResource AccentPurple}" />

    <SolidColorBrush x:Key="AccentOrangeBrush"
                     Color="{DynamicResource AccentOrange}" />

    <SolidColorBrush x:Key="AccentErrorBrush"
                     Color="{DynamicResource AccentError}" />

</ResourceDictionary>
