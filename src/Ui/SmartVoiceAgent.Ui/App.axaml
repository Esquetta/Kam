<Application
xmlns="https://github.com/avaloniaui"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
x:Class="SmartVoiceAgent.Ui.App"
RequestedThemeVariant="Dark">


	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude
				Source="avares://SmartVoiceAgent.Ui/Themes/AppTheme.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>


	<Application.Styles>
		<FluentTheme />
		<StyleInclude
		Source="avares://SmartVoiceAgent.Ui/Themes/Controls.axaml"/>
	</Application.Styles>


</Application>
